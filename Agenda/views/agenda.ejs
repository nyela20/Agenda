                                        <input type="text" name="nom" id="nom"
                                               value="<%= agenda.nom %>"
                                               class="w-full px-3.5 py-2.5 rounded-lg border border-primary placeholder-primary"
                                               required>
                                    </div>
                                    <div class="space-y-1.5">
                                        <label for="description" class="text-secondary">Description</label>
